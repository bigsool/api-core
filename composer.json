{
  "name": "api/core",
  "require": {
    "doctrine/annotations": "v1.2.4",
    "doctrine/cache": "v1.4.1",
    "doctrine/collections": "v1.3.0",
    "doctrine/common": "v2.5.0",
    "doctrine/dbal": "v2.5.1",
    "doctrine/inflector": "v1.0.1",
    "doctrine/instantiator": "1.0.4",
    "doctrine/lexer": "v1.0.1",
    "doctrine/migrations": "dev-master#67f0268",
    "doctrine/orm": "v2.5.0",
    "mandrill/mandrill": "1.0.54",
    "monolog/monolog": "1.13.1",
    "psr/log": "1.0.0",
    "symfony/config": "v2.7.1",
    "symfony/console": "v2.7.1",
    "symfony/filesystem": "v2.7.1",
    "symfony/http-foundation": "v2.7.1",
    "symfony/routing": "v2.7.1",
    "symfony/translation": "v2.7.1",
    "symfony/validator": "v2.7.1",
    "symfony/yaml": "v2.7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.7",
    "sami/sami": "~2.0",
    "guzzlehttp/guzzle": "~5.3"
  },
  "autoload": {
    "psr-0": {
      "Core": "src/",
      "Core\\Doctrine": "doctrine/",
      "Core\\Deploy": "deploy/",
      "Core\\Sami": "sami/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Core": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.0"
    }
  },
  "bin": [
    "bin/doctrine-generate-database",
    "bin/doctrine-generate-yml",
    "bin/generate-doc",
    "bin/phpunit-coverage",
    "bin/phpunit-test"
  ],
  "config": {
    "preferred-install": "dist"
  }
}
